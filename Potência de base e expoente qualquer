Algoritmo "Potencia_Manual"
Var
   base, expoente, resultado, i: inteiro
Inicio
   Escreva("Digite a base: ")
   Leia(base)
   Escreva("Digite o expoente: ")
   Leia(expoente)

   resultado <- 1

   Para i de 1 atÃ© expoente passo 1 faca
      resultado <- resultado * base
   FimPara

   Escreval("Resultado: ", resultado)
FimAlgoritmo
